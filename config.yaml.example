mode: priority-based  # or "round-robin"

# Global model configurations
models:
  claude-sonnet-4:
    top_p: 0.5
    temperature: 0.7
    max_tokens: 4096
  gemini-2.5:
    temperature: 0.9
    top_p: 0.8
  gpt-4o:
    temperature: 0.8
    frequency_penalty: 0.1
  claude-haiku:
    temperature: 0.5
    max_tokens: 2048

# Provider configurations (in priority order for priority-based mode)
providers:
  - name: Openrouter
    apiKeyEnvVar: OPENROUTER_API_KEY
    baseUrl: https://openrouter.ai/api/v1
    models:
      claude-sonnet-4: anthropic/claude-3.5-sonnet
      gemini-2.5: google/gemini-2.0-flash-exp
      gpt-4o: openai/gpt-4o
      claude-haiku: anthropic/claude-3-haiku
      
  - name: Nvidia
    apiKeyEnvVar: NVIDIA_API_KEY
    baseUrl: https://integrate.api.nvidia.com/v1
    models:
      claude-sonnet-4: nvidia/nemotron-4-340b-instruct
      gemini-2.5: nvidia/nemotron-4-340b-instruct
      gpt-4o: nvidia/nemotron-4-340b-instruct
      
  - name: OpenAI
    apiKeyEnvVar: OPENAI_API_KEY
    baseUrl: https://api.openai.com/v1
    models:
      gpt-4o: gpt-4o
      gpt-4o-mini: gpt-4o-mini
      gpt-4-turbo: gpt-4-turbo
      claude-sonnet-4: gpt-4o  # Fallback mapping
      
  - name: Anthropic
    apiKeyEnvVar: ANTHROPIC_API_KEY
    baseUrl: https://api.anthropic.com
    models:
      claude-sonnet-4: claude-3-5-sonnet-20241022
      claude-haiku: claude-3-haiku-20240307
      claude-opus: claude-3-opus-20240229
      gpt-4o: claude-3-5-sonnet-20241022  # Fallback mapping
